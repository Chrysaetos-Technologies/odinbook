= render "header"

=# render "users_list", title: @title, users: @users
.users-container
  .users-header
    %h1= @title
    %nav
      %ul
        %li= link_to "All Users", users_path
        %li= link_to "All Friends (#{current_user.friends.count})", friends_path(current_user)
        %li= link_to "Friend Requests", friend_requests_path(current_user)
        %li= link_to "Find Friends", find_friends_path(current_user)

  = render @users
